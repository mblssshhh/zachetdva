<Window x:Class="zachetdva.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zachetdva"
        mc:Ignorable="d"
        Title="Log in" Height="150" Width="300">
    <Grid>
        <StackPanel>
            <TextBox Name="Login" Width="100" Margin="3" TextChanged="TextBox_TextChanged" >

            </TextBox>
            <TextBox Name="Password" Width="100" TextChanged="TextBox_TextChanged_1" >
                
            </TextBox>
            <Button Width="100" Click="Button_Click">Log in</Button>
            <Label Width="100">Login: Markel</Label>
            <Label Width="100">Password: 4232</Label>
        </StackPanel>
    </Grid>
</Window>
